{
  "_meta": {
    "models": {
      "teams": "Groups of users for competency tracking, workforce management, and gap analysis; users can belong to multiple teams.",
      "users": "Individual employees who need certifications tracked; can have roles like User, Manager, Executive, or Auditor to determine their permissions and dashboard views.",
      "userCertifications": "A user's acquired certification with expiration date, upload proof, current status, renewal tracking, and verification timestamps.",
      "certifications": "A specific certification from the catalog with metadata including name, vendor, renewal cycle, intended audience (Developer, Architect, SRE, PM, etc.), prerequisites, and difficulty level.",
      "teamMetrics": "Aggregated dashboard metrics for each team showing certification coverage, expiring certifications, and competency gaps.",
      "certificationRequirements": "Required certifications for specific roles or teams to ensure compliance and competency standards.",
      "workforcePlanningInsights": "Strategic workforce planning data including projected gaps, hiring recommendations, and training priorities based on current team composition and requirements."
    },
    "relationships": [
      "Each Team contains multiple Users (through team membership)",
      "Each User has multiple UserCertifications showing their acquired certifications",
      "Each UserCertification references a Certification from the catalog",
      "Each Team has TeamMetrics showing aggregated coverage and gap data",
      "Each CertificationRequirement applies to a specific Team and Role combination",
      "WorkforcePlanningInsights are generated per Team based on requirements and current certifications"
    ]
  },
